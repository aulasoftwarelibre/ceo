<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">

    <link rel="apple-touch-icon-precomposed" href="{{ asset('apple-touch-icon.png') }}?v=2">
    <link rel="icon" sizes="32x32" href="{{ asset('favicon.ico') }}?v=2">
    <link rel="icon" sizes="256x256" href="{{ asset('favicon.png') }}?v=2">

    <title>{{ project_name | default('Servicio de Informatica') }} | {% block title%}{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('build/css/app.css') }}">
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}

{% block javascripts %}
    <script src="{{ asset('build/js/app.js') }}"></script>
{% endblock %}
</body>
</html>